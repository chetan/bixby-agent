#!/usr/bin/env ruby
# encoding: utf-8

require 'rubygems'
require 'bundler'

Dir.chdir(File.expand_path(File.join(File.dirname(__FILE__), ".."))) do
  Bundler.setup(:default)
end

$: << File.expand_path(File.join(File.dirname(__FILE__), "../lib"))

require 'bixby_agent'
require 'bixby_agent/app'

Bixby::App.new.run!
