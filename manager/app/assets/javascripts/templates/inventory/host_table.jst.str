
<div class="host_list">

  <% hosts.each(function(host) { %>

    <div class="host">
      <div class="body">
        <h3><a class="host" host_id="<%= host.get('id') %>"><%= host.name() %></a></h3>
      </div>
      <div class="actions"><a class="monitoring" host_id="<%= host.get('id') %>">Monitoring</a></div>
    </div>

  <% }); %>

</div>


