#!/usr/bin/env ruby

begin
  require "rubygems"
  require "bundler"
rescue Exception => ex
  if not Object.const_defined? :Bundler then
    # load from vendored path
    require 'pathname'
    $: << File.join(File.expand_path(File.dirname(Pathname.new(__FILE__).realpath)), "../vendor/cache/bundler-1.2.1/lib")
    require "bundler"
  end
end

require 'bundler/cli'
require 'bundler/friendly_errors'
Bundler.with_friendly_errors { Bundler::CLI.start }
